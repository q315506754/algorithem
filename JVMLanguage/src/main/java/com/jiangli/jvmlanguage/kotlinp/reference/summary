util:
    collection<T>:
        Iterable<T>:
            map(transform: (T) -> R): List<R>
            flatMap(transform: (T) -> Iterable<R>): List<R>
            filter(predicate: (T) -> Boolean): List<T>
            forEach(action: (T) -> Unit): Unit
